#include<bits/stdc++.h> 
using namespace std;
const int MAXN = 10005;
int dp[MAXN];
int n;
void mini() 
{ 
    
    for(int i=0;i<=MAXN;i++) dp[i]=i;
    for(int i = 1; i <= MAXN; i++)
    {
        for(int j = 1; j * j <= i; j++)
        {
            dp[i] = min(dp[i], 1 + dp[i - j*j]);
        }
    } 
} 

int main()
{
    int t;
    cin >> t;
    mini();
    while(t--)
    {
        cin >> n;
        cout << dp[n] << endl;
    }
    return 0;
}
