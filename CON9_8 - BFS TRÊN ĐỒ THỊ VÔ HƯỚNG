#include <bits/stdc++.h>
using namespace std;

typedef long long ll;
typedef unsigned long long ull;
typedef vector<vector<int> > vt;
const ll mod = 1e9 + 7;
const int oo = 1e6 + 7;
#define maxn 1005
int check[maxn];

void BFS(vector<int> adj[], int u)
{
	queue<int> q;
	q.push(u);
	check[u]=1;
	while(!q.empty())
	{
		int v=q.front();
		q.pop();
		cout << v << " ";
		for(int x:adj[v])
		{
			if(!check[x])
			{
				q.push(x);
				check[x]=1;
			}
		}
	}
}
int main() {
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	cout.tie(NULL);

    int t; cin >> t;
    while(t--)
	{
		vector<int> adj[maxn];
		int V, E, u;
		cin >> V >> E >> u;
		memset(check, 0, sizeof(check));
		for(int i=0;i<E;i++)
		{
			int x, y;
			cin >> x >> y;
			adj[x].push_back(y);
			adj[y].push_back(x);
		}
		BFS(adj, u);
		cout << '\n';
    }
    return 0;
}
