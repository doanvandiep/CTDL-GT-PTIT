#include <bits/stdc++.h>
using namespace std;

typedef long long ll;
typedef unsigned long long ull;
typedef vector<vector<int> > vt;
const ll mod = 1e9 + 7;
const int oo = 1e6 + 7;
#define maxn 1005
int main() {
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	cout.tie(NULL);

	int n;
	cin >> n;
	int a[n+1][n+1];
	memset(a, 0, sizeof(a));
	for(int i=1;i<=n;i++)
	{
		int x, y;
		cin >> x >> y;
		a[i][x]=1;
		a[i][y]=1;
	}
	for(int i=1;i<=n;i++)
	{
		for(int j=1;j<=n;j++)
		cout << a[i][j] << " ";
		cout << '\n';
	}
    return 0;
}
