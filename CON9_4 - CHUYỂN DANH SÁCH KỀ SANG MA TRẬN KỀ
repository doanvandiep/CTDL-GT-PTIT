#include <bits/stdc++.h>
using namespace std;

int main() {
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	cout.tie(NULL);
	int n;
	cin >> n;
	int a[n+1][n+1];
	memset(a, 0, sizeof(a));
	int x, y;
	for(int i=1;i<=n;i++)
	{
		cin >> x >> y;
		a[i][x]=1;
		a[i][y]=1;
	}
	for(int i=1;i<=n;i++)
	{
		for(int j=1;j<=n;j++) cout << a[i][j] << " ";
		cout << '\n';
	}
    return 0;
}
