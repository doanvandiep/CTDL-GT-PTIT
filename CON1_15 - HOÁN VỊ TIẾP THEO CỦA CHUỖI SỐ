#include <bits/stdc++.h>
using namespace std;

int main() {
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);cout.tie(NULL);
    int t; cin >> t;
    while(t--)
	{
		int n;
		string s;
		cin >> n >> s;
		cout << n << " ";
		if(next_permutation(s.begin(), s.end()))
		{
			cout << s << endl;
		}else
		{
			cout << "BIGGEST" << endl;
		}
    }
    return 0;
}
